{
  "compilerOptions": {
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": true
  },
  "exclude": ["node_modules"],
  "extends": "astro/tsconfigs/base",
  "include": ["**/*.ts", "**/*.tsx"]
}
