    "astro": "^3.0.7",
    "cheerio": "1.0.0-rc.12",
    "lodash": "4.17.21",
    "papaparse": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "twind": "^0.16.19"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.14.1",
    "@types/papaparse": "^5.3.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.15",
