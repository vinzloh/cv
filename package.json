{
  "name": "cv",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20",
    "pnpm": "^9.5.0"
  },
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "stale-dep && astro dev",
