    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.12.0",
    "stale-dep": "^0.7.0",
    "typescript": "^5.1.3"
